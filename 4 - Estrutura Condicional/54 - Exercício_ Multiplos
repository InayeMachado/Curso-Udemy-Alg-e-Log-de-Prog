Algoritmo "Multiplos"

Var
n1, n2: inteiro
operacao:real

Inicio
      escreval("Digite dois números inteiros")
      leia(n1)
      leia(n2)
      
      se (n1%n2) e (n2%n1) = 0 entao
         escreva("São multiplos")
         senao
         escreva("Não são multiplos")
      fimse
      
Fimalgoritmo
